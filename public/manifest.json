{
  "name": "FreeLog Highlighter Extension",
  "version": "1.0.0",
  "manifest_version": 3,
  "icons": {
    "16": "icons/16-icon.png",
    "48": "icons/48-icon.png",
    "128": "icons/128-icon.png"
  },
  "permissions": ["storage", "activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["content.css"]
    }
  ]
}
